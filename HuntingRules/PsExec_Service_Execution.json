{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspace": {
      "type": "String"
    }
  },
  "resources": [
    {
      "type": "Microsoft.OperationalInsights/workspaces/savedSearches",
      "apiVersion": "2020-08-01",
      "name": "[concat(parameters('workspace'), '/PsExec_Service_Execution')]",
      "location": "[resourceGroup().location]",
      "properties": {
        "eTag": "*",
        "displayName": "PsExec Service Execution",
        "category": "Hunting Queries",
        "query": "DeviceProcessEvents\n| where FolderPath =~ \"C:\\\\Windows\\\\PSEXESVC.exe\" or ProcessVersionInfoOriginalFileName =~ \"psexesvc.exe\"",
        "version": 1,
        "tags": [
          {
            "name": "description",
            "value": "Detects launch of the PSEXESVC service, which means that this system was the target of a psexec remote execut"
          },
          {
            "name": "tactics",
            "value": "Execution"
          }
        ]
      }
    }
  ]
}
